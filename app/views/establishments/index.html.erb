
<h2 class="featurette-heading" style="float:left;margin-left:1%;color:red;">Listing establishments </h2>

<h2 style="float:right;margin-right:1%;"> <%= link_to 'New', new_establishment_path %> </h2>


<div class="my-table-container">
  <% @establishments.each do |establishment| %>
    <div class="featurette">
        <h3 class="featurette-heading" style="color:red;"> <%= link_to establishment.name, establishment %></h3>
        <div class="field">
          <p>
           <%= establishment.key %>
          </p>
          <p>
           <%= establishment.address %>
           <%= establishment.city %>
           <%= establishment.state %>
           <%= establishment.zipcode %> </br>
           <%= establishment.email %> </br>
          </p>

<!--         <td><% link_to 'Show', establishment %></td>
        <td><% link_to 'Edit', edit_establishment_path(establishment) %></td>
        <td><% link_to 'Destroy', establishment, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
        </div>
        <hr class="featurette-divider"/>
    </div>
  <% end %>
</div>


<br>