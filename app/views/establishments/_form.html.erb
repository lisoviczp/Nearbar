
<div class="new-form">
  <%= form_for(@establishment) do |f| %>
    <% if @establishment.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@establishment.errors.count, "error") %> prohibited this establishment from being saved:</h2>

        <ul>
        <% @establishment.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>

    <div class="featurette">
      <h2 class="featurette-heading" style="color:red;">name</h2>
      <div class="field">
        <%= f.text_field :name %>
      </div>
      <div class="field">
        <%= f.label :key %><br>
        <%= f.text_field :key %>
      </div>
    </div>

    <div class="featurette">
      <h2 class="featurette-heading" style="color:red;">where are you</h2>
      <div class="field">
      <%= f.label :address %><br>
      <%= f.text_field :address %>
      </div>
      <div class="field">
        <%= f.label :city %><br>
        <%= f.text_field :city %>
      </div>
      <div class="field">
        <%= f.label :state %><br>
        <%= f.text_field :state %>
      </div>
      <div class="field">
        <%= f.label :zipcode %><br>
        <%= f.text_field :zipcode %>
      </div>
    </div>

    <div class="featurette">
      <h2 class="featurette-heading" style="color:red;">contact</h2>
      <div class="field">
        <%= f.label :email %><br>
        <%= f.text_field :email %>
      </div>
      <div class="actions">
        <%= f.submit %>
      </div>
      <%= link_to 'Back', establishments_path %>

    </div>
  <% end %>
</div>
